<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="KBM.Mobile.Views.UserProfileDetailPage" Title="{Binding Title}">
    <StackLayout Spacing="20" Padding="15">
        <Label Text="{Binding CurrentProfile.userName}" FontSize="Small"/>
        <Label Text="{Binding CurrentProfile.nama}" FontSize="Medium"/>
        <Label Text="{Binding CurrentProfile.tanggalLahir}" FontSize="Small"/>
        <Label Text="{Binding CurrentProfile.kategori}" FontSize="Small"/>
        <Label Text="{Binding CurrentProfile.telepon}" FontSize="Small"/>
        <Label Text="{Binding CurrentProfile.alamat}" FontSize="Small"/>
        <Label Text="{Binding CurrentProfile.email}" FontSize="Small"/>
        <Label Text="{Binding CurrentProfile.kelompok}" FontSize="Small"/>
    </StackLayout>
</ContentPage>